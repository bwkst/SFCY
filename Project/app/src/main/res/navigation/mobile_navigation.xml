<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_function">
    <!-- 初始画面 -->

    <fragment
        android:id="@+id/navigation_function"
        android:name="com.mainactivity.ui.function.FunctionFragment"
        android:label="小车使用"
        tools:layout="@layout/fragment_function" />
    <!-- 小车使用页面 -->

    <fragment
        android:id="@+id/successuse"
        android:name="com.mainactivity.ui.function.SuccessUse"
        android:label="小车使用"
        tools:layout="@layout/fragment_success_use" />
    <!-- 小车开启使用页面 -->

    <fragment
        android:id="@+id/countprice"
        android:name="com.mainactivity.ui.memberarea.CountPrice"
        android:label="小车使用"
        tools:layout="@layout/fragment_count_price" />
    <!-- 费用结算页面 -->

    <fragment
        android:id="@+id/pay"
        android:name="com.mainactivity.ui.function.Pay"
        android:label="小车使用"
        tools:layout="@layout/fragment_pay" />
    <!-- 付款页面 -->

    <fragment
        android:id="@+id/successwin"
        android:name="com.mainactivity.ui.function.SuccessUse"
        android:label="小车使用"
        tools:layout="@layout/fragment_success_use" />
    <!-- 小车开启奖励页面 -->

    <fragment
        android:id="@+id/countscore"
        android:name="com.mainactivity.ui.function.CountScore"
        android:label="小车使用"
        tools:layout="@layout/fragment_count_price" />
    <!-- 积分结算页面 -->

    <fragment
        android:id="@+id/failtoopen"
        android:name="com.mainactivity.ui.function.FailToOpen"
        android:label="小车使用"
        tools:layout="@layout/fragment_fail_to_open" />
    <!-- 小车开启失败页面 -->

    <fragment
        android:id="@+id/cscar"
        android:name="com.mainactivity.ui.cs.CsCar"
        android:label="小车使用"
        tools:layout="@layout/fragment_cs_car" />
    <!-- 车辆反馈页面 -->

    <fragment
        android:id="@+id/csprice"
        android:name="com.mainactivity.ui.cs.CsPrice"
        android:label="小车使用"
        tools:layout="@layout/fragment_cs_price" />
    <!-- 付款反馈页面 -->

    <fragment
        android:id="@+id/navigation_memberarea"
        android:name="com.mainactivity.ui.memberarea.MemberareaFragment"
        android:label="用户专区"
        tools:layout="@layout/fragment_memberarea" />
    <!-- 用户专区页面 -->

    <fragment
        android:id="@+id/mainpage"
        android:name="com.mainactivity.mainpage"
        android:label="用户专区"
        tools:layout="@layout/activity_main" />
    <!-- 主页面 -->

    <fragment
        android:id="@+id/registerpage"
        android:name="com.mainactivity.RegisterActivity"
        android:label="用户专区"
        tools:layout="@layout/activity_register" />
    <!-- 注册页面 -->

</navigation>